  <HediffDef>
    <hediffClass>HediffWithComps</hediffClass>
    <defName>IM_RageDrive</defName>
    <label>rage drive</label>
    <description>todo</description>
    <isBad>false</isBad>
    <scenarioCanAdd>false</scenarioCanAdd>
    <everCurableByItem>false</everCurableByItem>
    <initialSeverity>0.001</initialSeverity>
    <minSeverity>0.001</minSeverity>
    <maxSeverity>1</maxSeverity>
    <comps>
      <li Class="EBSGFramework.HediffCompProperties_SeverityChangeWhenDamaged">
        <severityChangeFactor>0.01</severityChangeFactor>
      </li>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-12.5</severityPerDay>
      </li>
    </comps>
    <stages>
      <li>
        <label>dormant</label>
        <minSeverity>0.001</minSeverity>
      </li>
      <li>
        <label>warming up</label>
        <minSeverity>0.15</minSeverity>
        <statOffsets>
          <MoveSpeed>0.6</MoveSpeed>
        </statOffsets>
        <statFactors>
          <MeleeDamageFactor>1.1</MeleeDamageFactor>
        </statFactors>
      </li>
      <li>
        <label>engaged</label>
        <minSeverity>0.35</minSeverity>
        <statOffsets>
          <MoveSpeed>1</MoveSpeed>
        </statOffsets>
        <statFactors>
          <MeleeCooldownFactor>0.90</MeleeCooldownFactor>
          <MeleeDamageFactor>1.25</MeleeDamageFactor>
        </statFactors>
      </li>
      <li>
        <label>fired up</label>
        <minSeverity>0.6</minSeverity>
        <statOffsets>
          <MoveSpeed>1.4</MoveSpeed>
        </statOffsets>
        <statFactors>
          <MeleeCooldownFactor>0.80</MeleeCooldownFactor>
          <MeleeDamageFactor>1.4</MeleeDamageFactor>
        </statFactors>
        <mentalStateGivers>
          <li>
            <mentalState>BerserkMechanoid</mentalState>
            <mtbDays>4</mtbDays>
          </li>
        </mentalStateGivers>
      </li>
      <li>
        <label>overdrive</label>
        <minSeverity>0.9</minSeverity>
        <statOffsets>
          <MoveSpeed>1.8</MoveSpeed>
        </statOffsets>
        <statFactors>
          <MeleeCooldownFactor>0.7</MeleeCooldownFactor>
          <MeleeDamageFactor>1.6</MeleeDamageFactor>
        </statFactors>
        <mentalStateGivers>
          <li>
            <mentalState>BerserkMechanoid</mentalState>
            <mtbDays>1</mtbDays>
          </li>
        </mentalStateGivers>
      </li>
    </stages>
  </HediffDef>

